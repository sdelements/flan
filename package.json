{
  "name": "@sdelements/flan",
  "description": "A tasty tool that lets you save, load and share postgres snapshots with ease.",
  "version": "0.1.0-alpha.7",
  "author": "Houssam Haidar",
  "bin": {
    "flan": "./bin/run"
  },
  "bugs": "https://github.com/sdelements/flan/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "chalk": "^4.1.1",
    "cli-ux": "^5.6.2",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.3.0",
    "globby": "^10",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "2.3.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "yarn.lock"
  ],
  "homepage": "https://github.com/sdelements/flan",
  "keywords": [
    "database",
    "postgres",
    "dump"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "flan",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "sdelements/flan",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install",
    "eslint": "eslint --ext .js,.ts, . --cache --max-warnings=0",
    "prettier": "prettier --write ."
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.{js,ts}": "eslint",
    "*.{js,json,ts,md}": "prettier --write"
  }
}
